{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "description": "nodejs-express-template",
  "type": "module",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node ./dist/index.js",
    "dev": "nodemon --watch './**/*.ts' --inspect=0.0.0.0:9227 --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' -r dotenv/config src/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "build": "tsc",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "maitienduy@gmail.com",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.3.4",
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "elastic-apm-node": "^3.42.0",
    "express": "^4.18.2",
    "fastest-validator": "^1.16.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.1",
    "jsdom": "^21.1.1",
    "json-stringify-safe": "^5.0.1",
    "kafkajs": "^2.2.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^4.14.0",
    "mongoose": "^6.9.1",
    "mysql2": "^3.1.2",
    "redis": "^4.6.4",
    "triple-beam": "^1.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.191",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "npm": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
